{
  "name": "react-social-network",
  "version": "1.0.0",
  "main": "app.js",
  "author": "Max Georgievsky <max.georgievsky@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node app.js",
    "server": "nodemon app.js",
    "client": "yarn --cwd client start",
    "client:test": "yarn --cwd client test",
    "server:install": "yarn install",
    "client:install": "yarn --cwd client install",
    "i": "yarn server:install && yarn client:install",
    "dev": "concurrently \"yarn server\" \"yarn client\""
  },
  "dependencies": {
    "@types/react-image-crop": "^8.1.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^6.2.0",
    "config": "^3.3.6",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "express-validator": "^6.12.0",
    "fs": "^0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.13.2",
    "multer": "^1.4.2",
    "path": "^0.12.7",
    "serve-static": "^1.14.1",
    "sharp": "^0.28.3",
    "shortid": "^2.2.16"
  }
}
